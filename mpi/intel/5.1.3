#%Module1.0#####################################################################
##
## mvapich modulefile
##
## modulefiles/modules.  Generated from modules.in by configure.
##
proc ModulesHelp { } {
	global version prefix

	puts stderr "\tIntel MPI 5.1.3"
}

conflict mpi/openmpi
conflict mpi/mvapich2

module-whatis	"Intel MPI 5.1.3"

# for Tcl script use only
set	version		5.1.3
set	prefix		/opt/intel/parallel_studio_xe_2016.3.067/compilers_and_libraries_2016.3.210/linux/mpi

setenv	MPI_ROOT	${prefix}
setenv 	I_MPI_ROOT 	${prefix}
prepend-path	PATH		${prefix}/intel64/bin
prepend-path	LIBRARY_PATH	${prefix}/intel64/lib
prepend-path	LD_LIBRARY_PATH	${prefix}/intel64/lib
prepend-path	MANPATH		${prefix}/man/man3

setenv	MPICC	${prefix}/intel64/bin/mpicc
setenv	MPICXX	${prefix}/intel64/bin/mpicxx
setenv	MPIF77	${prefix}/intel64/bin/mpif77
setenv	MPIF90	${prefix}/intel64/bin/mpif90

